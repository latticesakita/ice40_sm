int ov08x_start(struct gpio_instance *gpio_inst, unsigned char ov08x_slave)
{
	int ret = 0;
	int i;
	uint16_t sz;
	uint16_t offset;
	uint8_t dev_id[2];
	if(gpio_inst == NULL)
	{
		return -1;
	}
	i2c_init(gpio_inst, I2C_SYS_CLK_FREQ);

	gpio_output_write(gpio_inst, GPIO_OSC_EN,        GPIO_LOW);
	gpio_output_write(gpio_inst, GPIO_SENSOR_RESETN, GPIO_LOW);
	gpio_set_direction(gpio_inst, GPIO_OSC_EN,        GPIO_OUTPUT);
	gpio_set_direction(gpio_inst, GPIO_SENSOR_RESETN, GPIO_OUTPUT);
	gpio_output_write(gpio_inst, GPIO_OSC_EN,        GPIO_HIGH);
	usleep(10000); // wait 10ms to stablize the OSC output
	gpio_output_write(gpio_inst, GPIO_SENSOR_RESETN, GPIO_HIGH);
	usleep(5000); // wait 5ms to wait sensor to be reset properly

	for( i=0; i2c_data[i] != NULL; i++ ){
		sz = i2c_data[i][0];
		offset = i2c_data[i][1];
		offset = (offset<<8) | i2c_data[i][2];

		ret = i2c_write(ov08x_slave, offset, sz, &i2c_data[i][3]);
		if(ret < 0){
			return ret;
		}
	}
	return ret;
}

int ov08x_stop(struct gpio_instance *gpio_inst, unsigned char ov08x_slave)
{
	usleep(100); // wait 512 clock cycles, 100us is dummy
	gpio_output_write(gpio_inst, GPIO_OSC_EN,        GPIO_LOW);
	gpio_output_write(gpio_inst, GPIO_SENSOR_RESETN, GPIO_LOW);
	gpio_set_direction(gpio_inst, GPIO_OSC_EN,        GPIO_OUTPUT);
	gpio_set_direction(gpio_inst, GPIO_SENSOR_RESETN, GPIO_OUTPUT);
	return 0;
}

